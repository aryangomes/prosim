<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.br.prosim.prosim" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Main"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Processador"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_processador"
            android:screenOrientation="portrait" >
            >
        </activity>
        <activity
            android:name=".ModoHexadecimal"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/ModoHexadecimal"
            android:screenOrientation="portrait" >
            >
        </activity>
        <activity
            android:name=".HexaBinario"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/HexaBinario"
            android:screenOrientation="portrait" >
            >
        </activity>
        <activity
            android:name=".Sobre"
            android:label="@string/title_activity_sobre" >
        </activity>
    </application>

</manifest>
